{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "transparency",
      "camelcase": "Transparency",
      "title": "Transparency",
      "scope": "source.transparency",
      "file-types": ["t"],
      "injection-regex": "^transparency$",
      "class-name": "TreeSitterTransparency",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Transparency, a macro-dataflow language",
    "authors": [
      {
        "name": "Luddy Harrison",
        "email": "luddy@coreograph.com"
      }
    ],
    "links": {
      "repository": "https://github.com/AIR-Technology/tree-sitter-transparency"
    }
  },
  "bindings": {
    "transparency": true
  }
}
